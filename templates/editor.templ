package templates

import "app/config"

templ Editor() {
	<section>
		<div id="editorjs"></div>
		<script src={ config.AssetsPath + "js/editor.js" }></script>
	</section>
}

templ EditorHeader() {
	<nav class="p-4">
		<a href={ config.DashboardPath }>
			‚Üê Volver a Mis sitios
		</a>
	</nav>
	<header class="conex-banner">
		<img src={ config.RootPrefix + "assets/default-banner.svg" }/>
		<div class="conex-banner-content">
			<h1>Mi Sitio</h1>
			<p>Crear un sitio</p>
		</div>
	</header>
}
